public class SuaNhanVienView extends javax.swing.JFrame {
	NhanVienService nhanVienService = new NhanVienService();
	PhongBanService phongBanService = new PhongBanService();
	HDLDService hdldService = new HDLDService();
	ChucVuService chucVuService = new ChucVuService();

	/**
	 * Creates new form SuaNhanVienView
	 */
	public SuaNhanVienView(String maNhanVien) {
		setTitle("Sua Thong Tin Nhan Vien");
		initComponents();
		loadData(maNhanVien);

	}

	private void loadData(String maNhanVien) {
		Vector<String> phongBanData = new Vector<String>();
		for (PhongBan pb : phongBanService.findAll()) {
			phongBanData.add(pb.getTenPhongBan());
		}
		comboBoxPhongBan.setModel(new DefaultComboBoxModel<String>(phongBanData));
		NhanVien nv = nhanVienService.findById(maNhanVien);
		nv.setChucVu(chucVuService.findById(nv.getMaChucVu()));
		PhongBan pb = phongBanService.findById(nv.getMaPhongBan());
		HDLD hdld = hdldService.findById(nv.getMaHDLD());
		
		fieldIDNhanVien.setText(nv.getMaNhanVien());
		comboBoxChucVu.setSelectedItem(nv.getChucVu().getTenChucVu());
		fieldChuyenMon.setText(nv.getChuyenMon());
		fieldCMND.setText(nv.getCMND());
		fieldNgaySinh.setText(nv.getNgaySinh());
		fieldTenNhanVien.setText(nv.getTenNhanVien());
		fieldTrinhDo.setText(nv.getHocVan());
		fieldSDT.setText(nv.getSoDienThoai());
		if (nv.getGioiTinh().equalsIgnoreCase("Nam"))
			namRadio.setSelected(true);
		else
			nuRadio.setSelected(true);
		fieldHDLD.setText(hdld.getIdHDLD());
		comboBoxTypeHD.setSelectedItem(nv.getChucVu());
		fieldLuong.setText("" + hdld.getLuongCB());
		fieldHSLuong.setText("" + hdld.getHeSoLuong());
		fieldNgayBD.setText(hdld.getNgayBD());
		fieldNgayKT.setText(hdld.getNgayKT());
		comboBoxPhongBan.setSelectedItem(pb.getTenPhongBan());

	}

